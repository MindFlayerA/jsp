<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 


	<bean id="marks" class="com.springjdbc.SpringAutoWire.Marks">
		<property name="chem" value="80"></property>
		<property name="phy" value="90"></property>
		<property name="math" value="99"></property>
		<property name="java" value="90"></property>
			</bean>
			
			<bean id="st" class="com.springjdbc.SpringAutoWire.Student" autowire="byName">
				<property name="name" value="Amit"></property>
				<property name="email" value="amit@codeseeker.com"></property>
				<property name="roll" value="52532"></property>
					</bean>
			
			</beans>